{
  "stryker-config": {
    "coverage-analysis": "perTest",
    "thresholds": {
      "high": 100,
      "low": 90,
      "break": 90
    },
    "mutate": [
      "**/*.cs",
      "!**/Program.cs"
    ],
    "test-case-filter": "FullyQualifiedName!~Integration",
    "ignore-methods": [
      "*Exception.ctor",
      "Information",
      "Debug",
      "Delay"
    ]
  }
}
