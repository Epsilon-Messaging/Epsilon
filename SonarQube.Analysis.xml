<SonarQubeAnalysisProperties  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.sonarsource.com/msbuild/integration/2015/1">
    <Property Name="sonar.exclusions">**/obj/**, **/bin/**</Property>
    <Property Name="sonar.verbose">false</Property>
    <Property Name="sonar.projectName">Epsilon-Messaging</Property>
    <Property Name="sonar.analysis.projectTags">epsilon-messaging</Property>
    <Property Name="sonar.cs.opencover.reportsPaths">**/*opencover.xml</Property>
    <Property Name="sonar.coverage.exclusions">**/*.Tests/**,**/Program.cs</Property>
    <Property Name="sonar.pullrequest.github.repository">Epsilon-Messaging/Epsilon</Property>
    <Property Name="sonar.pullrequest.provider">github</Property>
</SonarQubeAnalysisProperties>